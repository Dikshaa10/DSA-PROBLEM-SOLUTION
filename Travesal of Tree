class Node{
int data ;
Node next ;
Node(int data){
this.data = data ;
Node left ,right = null ;
}
}
class Binary(Node root){
// preorder
if(root== null){
System.out.println(root.data + " ");
preorder(root.left);
preorder(root.right);

// inorder
if(root== null){
ineorder(root.left);
ineorder(root.right);
System.out.println(root.data +" ");
// Postorder 
if(root== null){
Postorder(root.left);
Postorder(root.right);
System.out.println(root.data + " ")
